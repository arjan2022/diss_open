@article{sirbu_politikat_2015,
	title = {Politikat kulturore dhe studimet folklorike ne {Shqiperine} komuniste},
	issn = {2223-8174, 2522-6126},
	abstract = {Even nowadays, folklorist studies compound one of the main disciplines in the post-communist countries. From a historical point of view, this is related with the process of national state building and as a consequence the study of folklore is instrumental zed and improved from the communist power to build its own strategy. Folkloric studies and traditional music especially, were taught in schools and universities as part of national culture. The way how are perceived and constructed these books emphasizes the idea of folklore as an ideological and political structure. We could say that the lessons of traditional music have influenced the culture of masses that was developed after the fall of communism. This article studies the period of communism when knowledge on folklore was instrumental zed in favor of the national state building.},
	language = {Albanian},
	number = {14},
